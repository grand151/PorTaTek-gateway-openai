# Konfiguracja serwera
PORT=8787

# Klucze API (wymagany przynajmniej jeden)
# OpenRouter API key (dla modeli: DeepSeek, Qwen, Mistral, Llama, Gemma, OpenCode, itp.)
OPENROUTER_API_KEY=twój-klucz-api-openrouter

# Google Gemini API key (dla modeli Gemini)
# Uzyskaj za darmo z: https://aistudio.google.com
GEMINI_API_KEY=twój-klucz-api-gemini

# Ustawienia cache
CACHE_TTL=3600000

# Logowanie
LOG_LEVEL=info

# Konfiguracja fallbacków i retry
MAX_RETRIES=3
RETRY_DELAY=1000 